{
  "name": "@vitruvian-tech/machete-bundle",
  "version": "1.0.0-pre.5",
  "description": "",
  "scripts": {
    "lint": "machete lint --bundle $(pwd)",
    "mount": "machete mount --bundle $(pwd)",
    "save": "machete save --bundle $(pwd)",
    "delete": "machete delete --bundle $npm_package_name",
    "install": "machete install --bundle $npm_package_name && npm run upgrade",
    "uninstall": "machete uninstall --bundle $npm_package_name",
    "upgrade": "machete migrate up --bundle $npm_package_name",
    "downgrade": "machete migrate down --bundle $npm_package_name"
  },
  "author": "Peter C. Romano <pete@vitruvian.tech> (http://github.com/VitruvianTech)",
  "license": "BSD-3-Clause",
  "dependencies": {
    "react-bootstrap-modal": "4.1.1",
    "react-share": "1.19.1"
  }
}
